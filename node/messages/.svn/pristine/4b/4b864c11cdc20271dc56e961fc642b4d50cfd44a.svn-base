#, kde-format
msgid "Finds devices whose name match :q:"
msgstr "Trovi aparaton kies nomo kongruas :q:"

#: solidrunner.cpp:49 solidrunner.cpp:137
#, kde-format
msgctxt "Note this is a KRunner keyword"
msgid "device"
msgstr "aparato"

#: solidrunner.cpp:50
#, kde-format
msgid "Lists all devices and allows them to be mounted, unmounted or ejected."
msgstr ""
"Listi ĉiujn aparatojn kaj ebligi ilin esti surmetitaj, demetitaj kaj "
"elĵetitaj."

#: solidrunner.cpp:51 solidrunner.cpp:143
#, kde-format
msgctxt "Note this is a KRunner keyword"
msgid "mount"
msgstr "surmeti"

#: solidrunner.cpp:52
#, kde-format
msgid "Lists all devices which can be mounted, and allows them to be mounted."
msgstr "Listi ĉiujn aparatojn kaj ebligi ilin esti surmetitaj."

